(deftest closure-materialized
  (is (= '[[(CLASS Vx [PUBLIC FINAL] "pkg/ns0/T$fn" nil "tinyclj/lang/AFnMh")
            [(FIELD [PRIVATE FINAL] "x" "Ljava/lang/String;")]
            
            [(METHOD [PUBLIC FINAL] "fn0" "()Ljava/lang/String;")
             ["LOCAL 0: T$fn fn"]
             (ALOAD_0)
             (GETFIELD . "x" "Ljava/lang/String;")
             (ARETURN)]
            
            [(METHOD [PUBLIC FINAL SYNTHETIC] "__arityOrNull"
                     "(I)Ljava/lang/invoke/MethodHandle;")
             #_["LOCAL 0: T$fn __this"]
             #_["LOCAL 1: int n"]
             (ILOAD_1)
             (IFNE L:0)
             (LDC [VIRTUAL . "fn0" "()Ljava/lang/String;"])
             (ARETURN)
             [L:0]
             (ACONST_NULL)
             (ARETURN)]
            
            [(METHOD [PUBLIC FINAL SYNTHETIC] "__directMethodHandles"
                     "()[Ljava/lang/invoke/MethodHandle;")
             #_["LOCAL 0: T$fn __this"]
             (ICONST_1)
             (ANEWARRAY "MethodHandle[]")
             (DUP)
             (ICONST_0)
             (LDC [VIRTUAL . "fn0" "()Ljava/lang/String;"])
             (AASTORE)
             (ARETURN)]
            
            [(METHOD [PRIVATE SYNTHETIC] "<init>"
                     "(Lclojure/lang/IPersistentMap;Ljava/lang/String;)V")
             #_["LOCAL 0: uninitialized_this_type __this"]
             #_["LOCAL 1: IPersistentMap __meta"]
             #_["LOCAL 2: String x"]
             (ALOAD_0)
             (ALOAD_2)
             (PUTFIELD . "x" "Ljava/lang/String;")
             (ALOAD_0)
             (ALOAD_1)
