(deftest data-only
  (is (= '[[(CLASS Vx [PUBLIC FINAL] "pkg/ns0/Vec3"
                   nil "java/lang/Object" [IType])
            [(FIELD [PUBLIC FINAL] "x" "F")]
            [(FIELD [PUBLIC FINAL] "y" "F")]
            [(FIELD [PUBLIC FINAL] "z" "F")]
            
            [(METHOD [PUBLIC SYNTHETIC] "<init>" "(FFF)V")
             #_["LOCAL 0: uninitialized_this_type __this"]
             #_["LOCAL 1: float x"]
             #_["LOCAL 2: float y"]
             #_["LOCAL 3: float z"]
             (ALOAD_0)
             (FLOAD_1)
             (PUTFIELD . "x" "F")
             (ALOAD_0)
             (FLOAD_2)
             (PUTFIELD . "y" "F")
             (ALOAD_0)
             (FLOAD_3)
             (PUTFIELD . "z" "F")
             (ALOAD_0)
             (INVOKESPECIAL Object "<init>" "()V")
             (RETURN)]]
           
           [(CLASS Vx [PUBLIC ABSTRACT] "pkg/ns0/_10")
            [(FIELD [PUBLIC STATIC VOLATILE] "a" "[Lpkg/ns0/Vec3;")]
            
            [(METHOD [PUBLIC STATIC] "<clinit>" "()V")
             (LDC "pkg.ns0")
             (INVOKESTATIC RT "createNamespace" "(Ljava/lang/String;)V")
             (RETURN)]]
           
           [(CLASS Vx [PUBLIC ABSTRACT] "pkg/ns0/___" nil "pkg/ns0/_10")
            [(METHOD [PUBLIC STATIC FINAL SYNTHETIC] "requires~1" "()Ljava/lang/String;")
             (LDC "clojure.core")
             (ARETURN)]]]
         (nmsp '[(ns pkg.ns0)
                 ;; Expansion of (deftype Vec3 [^float x ^float y
                 ;; ^float z]) minus the class import, the factory
                 ;; function, and the class reference.
                 (deftype*
                   pkg.ns0/Vec3
                   pkg.ns0.Vec3
                   [^float x ^float y ^float z]
                   :implements
                   [clojure.lang.IType])
                 (def ^pkg.ns0.Vec3/1 a)]))))

